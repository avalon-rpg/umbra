{
  "name": "avalon-rpg.shadow-client",
  "version": "0.0.0",
  "description": "A pure HTML web client for Avalon's shadow protocol",
  "main": "server/app.js",
  "author": "Kevin Wright",
  "private": true,
  "license": "avalon-rpg",
  "scripts": {
    "once": "node --harmony ./build/server/app.js",
    "start": "pm2 start ./build/server/app.js --node-args=\"--harmony\"",
    "stop": "pm2 stop ./build/server/app.js --node-args=\"--harmony\"",
    "restart": "pm2 restart ./build/server/app.js --node-args=\"--harmony\"",
    "test": "mocha --harmony -R spec --recursive ./build/test/*.js"
  },
