{
  "name": "avalon-rpg.shadow-client",
  "version": "0.2.0",
  "description": "A pure HTML web client for Avalon's shadow protocol",
  "main": "server/app.js",
  "author": "Kevin Wright",
  "private": true,
  "license": "avalon-rpg",
  "scripts": {
    "once": "grunt && node ./build/server/app.js",
    "startdev": "set NODE_ENV=dev&& pm2 start ./build/server/app.js --name umbradev",
    "stopdev": "set NODE_ENV=dev&& pm2 stop umbradev",
    "start": "set NODE_ENV=production&& pm2 start ./build/server/app.js --name umbra",
    "stop": "set NODE_ENV=production&& pm2 stop umbra",
    "test": "mocha -R spec --recursive ./build/test/*.js"
  },
  "dependencies": {
    "CBuffer": "^2.0.0",
    "ansi_up": "^1.3.0",
    "babel-cli": "^6.24.1",
    "binary": "^0.3.0",
    "body-parser": "^1.17.1",
    "buffertools": "^2.1.6",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mobile-detect": "^1.3.6",
    "murmur": "^0.0.2",
    "pm2": "^2.10.2",
    "pmx": "^1.2.0",
    "q": "^1.5.0",
    "socket.io": "^2.0.1",
    "strip-ansi": "^3.0.1",
    "uws": "^9.14.0"
  },
  "devDependencies": {
    "babel-plugin-closure-elimination": "1.1.16",
    "babel-preset-es2015": "^6.24.1",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-less": "^1.4.1",
    "grunt-contrib-uglify": "^3.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-riot": "^0.1.8",
    "less-plugin-autoprefix": "^1.5.1"
  }
}
