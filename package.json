    "cheerio": "0.18.0",
    "compression": "^1.4.1",
    "express": "^4.12.0",
    "jade": "1.9.2",
    "jquery": "1.11.2",
    "mobile-detect": "^0.4.3",
    "murmur": "0.0.2",
    "pm2": "^0.12.6",
    "pmx": "^0.3.5",
    "q": "^1.2.0",
    "socket.io": "1.3.4",
    "strip-ansi": "2.0.1",
    "watchr": "2.4.13"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
