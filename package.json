{
  "name": "avalon-rpg.shadow-client",
  "version": "0.0.0",
  "description": "A pure HTML web client for Avalon's shadow protocol",
  "main": "server/app.js",
  "author": "Kevin Wright",
  "private": true,
  "license": "avalon-rpg",
  "scripts": {
    "once": "grunt && iojs ./build/server/app.js",
    "startdev": "set NODE_ENV=dev&& pm2 start ./build/server/app.js --name umbradev --interpreter iojs",
    "stopdev": "set NODE_ENV=dev&& pm2 stop umbradev --interpreter iojs",
    "start": "set NODE_ENV=production&& pm2 start ./build/server/app.js --name umbra --interpreter iojs",
    "stop": "set NODE_ENV=production&& pm2 stop umbra --interpreter iojs",
    "test": "mocha -R spec --recursive ./build/test/*.js"
  },
  "dependencies": {
    "CBuffer": "^1.1.0",
    "ansi_up": "^1.1.3",
    "babel": "^5.1.13",
    "binary": "^0.3.0",
    "body-parser": "^1.12.3",
    "buffertools": "^2.1.2",
    "chai": "^2.3.0",
    "cheerio": "^0.19.0",
    "compression": "^1.4.3",
    "express": "^4.12.3",
    "jade": "^1.9.2",
    "jquery": "^1.11.2",
    "lodash": "^3.7.0",
    "mobile-detect": "^1.0.0",
    "murmur": "^0.0.2",
    "pako": "^0.2.6",
    "pm2": "^0.12.13",
    "pmx": "^0.3.9",
    "q": "^1.3.0",
    "socket.io": "^1.3.5",
    "strip-ansi": "^2.0.1",
    "watchr": "^2.4.13"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-concat-sourcemaps": "^0.6.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-jade": "^0.14.1",
    "grunt-contrib-less": "^1.0.1",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-riot": "^0.0.17",
    "less-plugin-autoprefix": "^1.4.1"
  }
}
